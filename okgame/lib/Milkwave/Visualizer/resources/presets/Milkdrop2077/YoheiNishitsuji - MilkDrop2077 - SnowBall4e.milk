warp_1=`shader_body{
warp_2=`
warp_3=`//Shader from https://x.com/YoheiNishitsuji/status/1923362809569837131
warp_4=`//Ported to MilkDrop by MilkDrop2077
warp_5=`
warp_6=`    float aspect = aspect.x / aspect.y;
warp_7=`    float2 u = (uv - 0.5) * float2(aspect, 1.0) * 2.0;
warp_8=`    u = -u; //Correctly flip UV
warp_9=`    float4 o = float4(0.0, 0.0, 0.0, 0.0);
warp_10=`    float t = time;
warp_11=`    float g = 0.0;
warp_12=`    
warp_13=`    for(float i = 0.0; i < 99.0; i += 1.0) {
warp_14=`        float3 p = float3(4.5 * u.x, 4.5 * u.y, g - 7.0);
warp_15=`        float3 A = float3(0.0, 9.0, -3.0)/9.5;
warp_16=`        p.y += 1.0;
warp_17=`        
warp_18=`        // 3D rotation
warp_19=`        p = A * dot(p+p, A) - p - 0.14*cross(p, A);
warp_20=`        
warp_21=`        // 2D rotation on ZY plane
warp_22=`        float angle = t * 0.92;
warp_23=`        float2x2 rot = transpose(float2x2(
warp_24=`            cos(angle), -sin(angle),
warp_25=`            sin(angle), cos(angle)
warp_26=`        )); // In GLSL is column-major order and in HLSL, row-major order,
warp_27=`// So we transpose the matrix with transpose() to be the same as GLSL.
warp_28=`        p.zy = mul(p.zy, rot);
warp_29=`        
warp_30=`        float s = 6.0;
warp_31=`        float e;
warp_32=`        
warp_33=`        // Modified fractal iteration with time-based variation
warp_34=`        for(float j = 0.0; j < 7.0; j += 1.0) {
warp_35=`            e = 18.0 / dot(p, p);
warp_36=`            s *= e;
warp_37=`            float xSize = 3.0 + 2.5 * sin(t * 0.5);
warp_38=`            p = float3(0.0, 4.213, 0.0) - abs(abs(p)*e - float3(xSize, 4.0, 3.0));
warp_39=`        }
warp_40=`        
warp_41=`        g += 0.2 * p.y * p.y / s;
warp_42=`        o += (log2(s) - g * 0.7) / 700.0 * float4(0.9, 1.0, 1.0, 0.0);
warp_43=`    }
warp_44=`    
warp_45=`    ret = float4(tanh(o.x * 2.0), tanh(o.y * 2.0), tanh(o.z * 2.0), 1.0);
warp_46=`}
