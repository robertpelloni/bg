comp_130=float3 grad(float3 p){
comp_131=  const float2 e = float2(.005,0);
comp_132=  float a =map(p).x;
comp_133=  return float3(map(p+e.xyy).x-a
comp_134=  ,map(p+e.yxy).x-a
comp_135=  ,map(p+e.yyx).x-a)/e.x;
comp_136=}
