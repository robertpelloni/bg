comp_28=float jTime;
comp_29=
comp_30=#ifdef disable_sound_texture_sampling
comp_31=#define textureMirror(a, b) float4(0,0,0,0)
comp_32=
comp_33=#else
comp_34=float4 textureMirror(sampler2D tex, float2 c){
comp_35=  float2 cf = frac(c);
