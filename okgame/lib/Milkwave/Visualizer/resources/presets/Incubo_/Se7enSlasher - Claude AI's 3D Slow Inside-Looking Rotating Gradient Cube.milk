per_frame_init_1=randdiv1 = rand(1)/2;
per_frame_init_2=randdiv2 = rand(1)/2;
per_frame_init_3=randdiv3 = rand(1)/2;
per_frame_1=//Cube Rotation - Now with randomized time movement + Optimization
per_frame_2=rot_x = rot_x + 0.005*sin(time*(1*randdiv1)); // Smooth X rotation
per_frame_3=rot_y = rot_y + 0.005*sin(time*(1*randdiv2)); // Smooth Y rotation
per_frame_4=rot_z = rot_z + 0.005*sin(time*(1*randdiv3)); // Smooth Z rotation
per_frame_5=
per_frame_6=//Transfer variables to q in use of shaders.
per_frame_7=q1 = rot_x;
per_frame_8=q2 = rot_y;
per_frame_9=q3 = rot_z;
warp_1=`shader_body
warp_2=`{
warp_3=`    // Create rotation matrices
warp_4=`    float3x3 rotX = float3x3(
warp_5=`        1, 0, 0,
warp_6=`        0, cos(q1), -sin(q1),
warp_7=`        0, sin(q1), cos(q1)
warp_8=`    );
warp_9=`    
warp_10=`    float3x3 rotY = float3x3(
warp_11=`        cos(q2), 0, sin(q2),
warp_12=`        0, 1, 0,
warp_13=`        -sin(q2), 0, cos(q2)
warp_14=`    );
warp_15=`    
warp_16=`    float3x3 rotZ = float3x3(
warp_17=`        cos(q3), -sin(q3), 0,
warp_18=`        sin(q3), cos(q3), 0,
warp_19=`        0, 0, 1
warp_20=`    );
warp_21=`    
warp_22=`    // Create cube vertices
warp_23=`    float3 pos = float3(uv.x * 2 - 1, uv.y * 2 - 1, 1);
warp_24=`    pos.z = .5;  // Set initial Z position
warp_25=`    
warp_26=`    // Apply rotations
warp_27=`    pos = mul(rotX, pos);
warp_28=`    pos = mul(rotY, pos);
warp_29=`    pos = mul(rotZ, pos);
warp_30=`    
warp_31=`    // Project to screen space
warp_32=`    ret = float3(pos.xy / (pos.z + 2), pos.z);
warp_33=`
warp_34=`    // Calculate face normal for texturing
warp_35=`    float3 normal = normalize(pos);
warp_36=`    
warp_37=`    // Generate unique texture per face based on normal direction
warp_38=`    float3 color;
warp_39=`    if (abs(normal.x) > abs(normal.y) && abs(normal.x) > abs(normal.z)) {
warp_40=`        // X-facing sides
warp_41=`        color = lerp(
warp_42=`            float3(1,0,0), 
warp_43=`            float3(0,1,0), 
warp_44=`            (sin(time*0.1 + uv.x*3) + 1)*0.5
warp_45=`        );
warp_46=`    } else if (abs(normal.y) > abs(normal.z)) {
warp_47=`        // Y-facing sides
warp_48=`        color = lerp(
warp_49=`            float3(0,1,0), 
warp_50=`            float3(0,0,1), 
warp_51=`            (sin(time*0.15 + uv.y*3) + 1)*0.5
warp_52=`        );
warp_53=`    } else {
warp_54=`        // Z-facing sides
warp_55=`        color = lerp(
warp_56=`            float3(1,0,1), 
warp_57=`            float3(1,1,0), 
warp_58=`            (sin(time*0.2 + dot(uv,uv)*3) + 1)*0.5
warp_59=`        );
warp_60=`    }
warp_61=`    
warp_62=`    // Add audio reactivity
warp_63=`    //color *= 1 + bass*0.5;
warp_64=`    //color += mid*0.3;
warp_65=`    
warp_66=`    // Add depth-based shading
warp_67=`    float depth = (pos.z + 2) * 0.25;
warp_68=`    color *= depth;
warp_69=`
warp_70=`    ret = color;
warp_71=`}
comp_1=`shader_body
