per_frame_init_1=randdiv1 = rand(1)/2;
per_frame_init_2=randdiv2 = rand(1)/2;
per_frame_init_3=randdiv3 = rand(1)/2;
per_frame_1=//Cube Rotation - Now with randomized time movement + Optimization
per_frame_2=rot_x = rot_x + 0.0075*sin(time*(1*randdiv1)); // Smooth X rotation
per_frame_3=rot_y = rot_y + 0.0075*sin(time*(1*randdiv2)); // Smooth Y rotation
per_frame_4=rot_z = rot_z + 0.0075*sin(time*(1*randdiv3)); // Smooth Z rotation
per_frame_5=
per_frame_6=//Transfer variables to q in use of shaders.
per_frame_7=q1 = rot_x;
per_frame_8=q2 = rot_y;
per_frame_9=q3 = rot_z;
per_frame_10=
per_frame_11=//Se7enSlasher - My Slow-Decrease Bass Code
per_frame_12=oldbass += bass*2;
per_frame_13=oldbass = if (oldbass < bass, oldbass, oldbass/=1.025);
per_frame_14=q4 = 0.5 - oldbass/475;
per_frame_15=q5 = oldbass/450;
warp_1=`sampler sampler_rand00;  // Right face
warp_2=`sampler sampler_rand01;  // Left face
warp_3=`sampler sampler_rand02;  // Bottom face
warp_4=`sampler sampler_rand03;  // Top face
warp_5=`sampler sampler_rand04;  // Front face
warp_6=`sampler sampler_rand05;  // Back face
warp_7=`
warp_8=`shader_body
warp_9=`{
warp_10=`    // Create rotation matrices
warp_11=`    float3x3 rotX = float3x3(
warp_12=`        1, 0, 0,
warp_13=`        0, cos(q1), -sin(q1),
warp_14=`        0, sin(q1), cos(q1)
warp_15=`    );
warp_16=`    
warp_17=`    float3x3 rotY = float3x3(
warp_18=`        cos(q2), 0, sin(q2),
warp_19=`        0, 1, 0,
warp_20=`        -sin(q2), 0, cos(q2)
warp_21=`    );
warp_22=`    
warp_23=`    float3x3 rotZ = float3x3(
warp_24=`        cos(q3), -sin(q3), 0,
warp_25=`        sin(q3), cos(q3), 0,
warp_26=`        0, 0, 1
warp_27=`    );
warp_28=`    
warp_29=`    // Create cube vertices with proper aspect ratio
warp_30=`    float3 pos = float3((uv.x - 0.5) * 2, (uv.y - 0.5) * 2, q4);
warp_31=`    pos.x *= aspect;  // Correct for aspect ratio
warp_32=`    
warp_33=`    // Apply rotations
warp_34=`    pos = mul(rotX, pos);
warp_35=`    pos = mul(rotY, pos);
warp_36=`    pos = mul(rotZ, pos);
warp_37=`    
warp_38=`    // Project to screen space
warp_39=`    ret = float3(pos.xy / (pos.z + 2), pos.z);
warp_40=`    
warp_41=`    float3 absPos = abs(pos);
warp_42=`    float3 color;
warp_43=`    float2 texCoord;
warp_44=`    
warp_45=`    // Corrected UV mapping for each face
warp_46=`    if (absPos.x > absPos.y && absPos.x > absPos.z) {
warp_47=`        // X-facing sides (Right/Left)
warp_48=`        texCoord = float2(
warp_49=`            (pos.z / absPos.x + 1) * 0.5,
warp_50=`            (pos.y / absPos.x + 1) * 0.5
warp_51=`        );
warp_52=`        if (pos.x > 0) {
warp_53=`            texCoord.x = 1 - texCoord.x;  // Flip X for right face only
warp_54=`            color = tex2D(sampler_rand00, texCoord).rgb;
warp_55=`        } else {
warp_56=`            color = tex2D(sampler_rand01, texCoord).rgb;
warp_57=`        }
warp_58=`    }
warp_59=`    else if (absPos.y > absPos.z) {
warp_60=`        // Y-facing sides (Top/Bottom)
warp_61=`        texCoord = float2(
warp_62=`            (pos.x / absPos.y + 1) * 0.5,
warp_63=`            (pos.z / absPos.y + 1) * 0.5
warp_64=`        );
warp_65=`        if (pos.y > 0) {
warp_66=`            texCoord.y = 1 - texCoord.y; //Flip Y for bottom face only - Se7enSlasher
warp_67=`            color = tex2D(sampler_rand02, texCoord).rgb;
warp_68=`        } else {
warp_69=`            color = tex2D(sampler_rand03, texCoord).rgb;
warp_70=`        }
warp_71=`    }
warp_72=`    else {
warp_73=`        // Z-facing sides (Front/Back)
warp_74=`        texCoord = float2(
warp_75=`            (pos.x / absPos.z + 1) * 0.5,
warp_76=`            (pos.y / absPos.z + 1) * 0.5
warp_77=`        );
warp_78=`        if (pos.z > 0) {
warp_79=`            color = tex2D(sampler_rand04, texCoord).rgb;
warp_80=`        } else {
warp_81=`            texCoord.x = 1 - texCoord.x;  // Flip X for back face only
warp_82=`            color = tex2D(sampler_rand05, texCoord).rgb;
warp_83=`        }
warp_84=`    }
warp_85=`    
warp_86=`    // Add depth-based shading
warp_87=`    float depth = (pos.z + 2) * (0.10+q5);
warp_88=`    color *= depth;
warp_89=`    ret = color;
warp_90=`}
